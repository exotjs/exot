{
  "name": "@exotjs/exot",
  "version": "0.1.1",
  "description": "A cross-runtime TypeScript framework for server applications.",
  "author": "Daniel Regeci",
  "license": "MIT",
  "keywords": [
    "server",
    "http",
    "rest",
    "restful",
    "router",
    "api",
    "framework",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "denoify": "rimraf deno_dist && denoify && rimraf 'deno_dist/**/*.test.{ts,tsx}' && find deno_dist/. -type f -exec sed -i '' -e 's/node:node:/node:/g' {} +",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters/bun": {
      "types": "./dist/adapters/bun.d.ts",
      "import": "./dist/adapters/bun.js"
    },
    "./adapters/fetch": {
      "types": "./dist/adapters/fetch.d.ts",
      "import": "./dist/adapters/fetch.js"
    },
    "./adapters/node": {
      "types": "./dist/adapters/node.d.ts",
      "import": "./dist/adapters/node.js"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "import": "./dist/env.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js"
    },
    "./headers": {
      "types": "./dist/headers.d.ts",
      "import": "./dist/headers.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js"
    },
    "./pubsub": {
      "types": "./dist/pubsub.d.ts",
      "import": "./dist/pubsub.js"
    },
    "./request": {
      "types": "./dist/request.d.ts",
      "import": "./dist/request.js"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "import": "./dist/validation.js"
    },
    "./websocket": {
      "types": "./dist/websocket.d.ts",
      "import": "./dist/websocket.js"
    }
  },
  "typesVersions": {
    "*": {
      "adapters/bun": [
        "./dist/adapters/bun"
      ],
      "adapters/fetch": [
        "./dist/adapters/fetch"
      ],
      "adapters/node": [
        "./dist/adapters/node"
      ],
      "env": [
        "./dist/env"
      ],
      "errors": [
        "./dist/errors"
      ],
      "headers": [
        "./dist/headers"
      ],
      "helpers": [
        "./dist/helpers"
      ],
      "pubsub": [
        "./dist/pubsub"
      ],
      "request": [
        "./dist/request"
      ],
      "types": [
        "./dist/types"
      ],
      "validation": [
        "./dist/validation"
      ],
      "websocket": [
        "./dist/websocket"
      ]
    }
  },
  "dependencies": {
    "@sinclair/typebox": "^0.31.23",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "cookie": "^0.6.0",
    "fast-querystring": "^1.1.2",
    "find-my-way": "^7.7.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.4",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.10",
    "denoify": "^1.6.6",
    "rimraf": "^5.0.5",
    "tinybench": "^2.5.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.1",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
